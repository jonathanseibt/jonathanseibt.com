    "color": "4.2.3",
    "http-status-codes": "2.3.0",
    "lodash": "4.17.21",
    "next": "13.5.5-canary.4",
    "react": "18.2.0",
    "react-dom": "18.2.0"
  },
  "devDependencies": {
