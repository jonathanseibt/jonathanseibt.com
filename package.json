{
  "name": "jonathanseibt.com",
  "private": true,
  "scripts": {
    "build": "next build",
    "check-types": "tsc --noEmit",
    "deploy": "npx vercel --prod",
    "dev": "next dev",
    "eslint": "npx eslint . --fix --ext .js,.jsx,.ts,.tsx,.json",
    "lint": "npx sort-package-json && npm run eslint && npm run prettier && npm run check-types",
    "prettier": "npx prettier . --write",
    "preview": "npx vercel",
    "start": "next start"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "package.json": [
      "bash -c 'npx sort-package-json'"
    ],
    "*": [
      "bash -c 'npm run eslint'",
      "bash -c 'npm run prettier'",
      "bash -c 'npm run check-types'"
    ]
  },
  "dependencies": {
    "@material-ui/core": "^4.11.3",
    "classnames": "^2.2.6",
    "clean-css": "^5.1.2",
    "http-status-codes": "^2.1.4",
    "next": "10.1.2",
    "react": "17.0.2",
    "react-dom": "17.0.2"
  },
  "devDependencies": {
    "@types/classnames": "^2.2.11",
    "@types/clean-css": "^4.2.3",
    "@types/eslint": "^7.2.8",
    "@types/http-status-codes": "^1.2.0",
    "@types/prettier": "^2.2.3",
    "@types/react": "^17.0.3",
    "@types/react-dom": "^17.0.3",
    "@typescript-eslint/eslint-plugin": "^4.20.0",
    "@typescript-eslint/parser": "^4.20.0",
    "eslint": "^7.23.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "husky": "^6.0.0",
    "lint-staged": "^10.5.4",
    "prettier": "^2.2.1",
    "typescript": "^4.2.3"
  }
}
