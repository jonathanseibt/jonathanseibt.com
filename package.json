{
  "name": "jonathanseibt.com",
  "private": true,
  "scripts": {
    "prebuild": "yarn next-telemetry-disable",
    "build": "next build",
    "check-types": "tsc --noEmit",
    "ci": "yarn install --frozen-lockfile",
    "deploy": "vercel --prod",
    "dev": "next dev",
    "eslint": "eslint . --fix --ext .js,.jsx,.ts,.tsx,.json",
    "postinstall": "yarn next-telemetry-disable",
    "lint": "sort-package-json && yarn eslint && yarn prettier && yarn check-types",
    "next-telemetry-disable": "next telemetry disable",
    "prettier": "prettier . --write --loglevel error",
    "preview": "vercel",
    "setup": "yarn global add next vercel eslint prettier sort-package-json && husky install",
    "start": "next start"
  },
  "lint-staged": {
    "*": [
      "bash -c 'yarn lint'"
    ]
  },
  "dependencies": {
    "@material-ui/core": "^4.11.4",
    "classnames": "^2.3.1",
    "clean-css": "^5.1.2",
    "http-status-codes": "^2.1.4",
    "next": "11.1.0",
    "react": "17.0.2",
    "react-dom": "17.0.2"
  },
  "devDependencies": {
    "@types/classnames": "^2.3.1",
    "@types/clean-css": "^4.2.4",
    "@types/react": "^17.0.8",
    "@types/react-dom": "^17.0.5",
    "@typescript-eslint/eslint-plugin": "^4.25.0",
    "@typescript-eslint/parser": "^4.25.0",
    "eslint": "^7.27.0",
    "eslint-plugin-import": "^2.23.3",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "husky": "^6.0.0",
    "lint-staged": "^11.0.0",
    "prettier": "^2.3.0",
    "typescript": "^4.3.2"
  }
}
