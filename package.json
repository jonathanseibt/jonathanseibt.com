{
  "name": "jonathanseibt.com",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "prebuild": "npm run lint && npx next telemetry disable",
    "build": "next build",
    "check-types": "tsc --noEmit",
    "dev": "next dev",
    "eslint": "npx eslint . --fix --ext .js,.jsx,.ts,.tsx,.json",
    "postinstall": "npx next telemetry disable && npm run lint",
    "lint": "npx ts-typie && npx sort-package-json && npm run eslint && npm run prettier && npm run check-types",
    "prettier": "npx prettier . --write",
    "start": "next start"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "package.json": [
      "bash -c 'npx ts-typie'",
      "bash -c 'npx sort-package-json'"
    ],
    "*": [
      "bash -c 'npm run eslint'",
      "bash -c 'npm run prettier'",
      "bash -c 'npm run check-types'"
    ]
  },
  "dependencies": {
    "next": "10.1.2",
    "react": "17.0.2",
    "react-dom": "17.0.2"
  },
  "devDependencies": {
    "@types/eslint": "^7.2.8",
    "@types/prettier": "^2.2.3",
    "@types/react": "^17.0.3",
    "@types/react-dom": "^17.0.3",
    "@typescript-eslint/eslint-plugin": "^4.20.0",
    "@typescript-eslint/parser": "^4.20.0",
    "eslint": "^7.23.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "husky": "^6.0.0",
    "lint-staged": "^10.5.4",
    "prettier": "^2.2.1",
    "typescript": "^4.2.3"
  }
}
